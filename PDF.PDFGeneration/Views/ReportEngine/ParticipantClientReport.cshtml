@using Tiller.WXL.PDFGeneration
@model Tiller.WXL.Models.ViewModels.PrimaryParticipantSummaryViewModel

@{
    ViewBag.Title = "Client Report - Participant PDF";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Test</title>
    @if (Model.Mode == 2)
    {
        <link rel="stylesheet" href="~/css/bootstrap.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/font-awesome/css/font-awesome.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/custom/client-report/client-report.css" crossorigin="anonymous">
    }
    else
    {
        <link rel="stylesheet" href="css/bootstrap.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="css/font-awesome/css/font-awesome.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="css/custom/client-report/client-report.css" crossorigin="anonymous">
    }
</head>

<body>
    <div>
        <div class="main-container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="content-section">
                        <div class="main-heading">
                            <h1>Contents</h1>
                        </div>

                        <table class="table-of-content">
                            <colgroup>
                                <col style="width: 40%">
                                <col style="width: 50%">
                                <col style="width: 10%">
                            </colgroup>
                            <tbody>
                                @{
                                    var c = 1;
                                }
                                <tr>
                                    <td>@c</td>
                                    <td>
                                        <a href="#TOC_Target0" id="TOC_Title0" class="toc contents-page-style"><h4>Personal details</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number0" class="toc"></td>
                                </tr>
                                <tr>
                                    @{
                                        c += 1;
                                    }
                                    <td>@c</td>
                                    <td>
                                        <a href="#TOC_Target1" id="TOC_Title1" class="toc contents-page-style"><h4>Tax and employment</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number1" class="toc"></td>
                                </tr>
                                @if (Model.IndividualDetails.ParticipantMainDetail.IsMainApplicant)
                                {
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target2" id="TOC_Title2" class="toc contents-page-style"><h4>Third parties</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number2" class="toc"></td>
                                    </tr>
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target3" id="TOC_Title3" class="toc contents-page-style"><h4>Income</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number3" class="toc"></td>
                                    </tr>
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target4" id="TOC_Title4" class="toc contents-page-style"><h4>Assets and liabilities</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number4" class="toc"></td>
                                    </tr>
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target5" id="TOC_Title5" class="toc contents-page-style"><h4>Financial dependants</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number5" class="toc"></td>
                                    </tr>
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target6" id="TOC_Title6" class="toc contents-page-style"><h4>Source of funds</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number6" class="toc"></td>
                                    </tr>
                                    <tr>
                                        @{
                                            c += 1;
                                        }
                                        <td>@c</td>
                                        <td>
                                            <a href="#TOC_Target7" id="TOC_Title7" class="toc contents-page-style"><h4>Account information</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number7" class="toc"></td>
                                    </tr>
                                }
                                <tr>
                                    @{
                                        c += 1;
                                    }
                                    <td>@c</td>
                                    <td>
                                        <a href="#TOC_Target8" id="TOC_Title8" class="toc contents-page-style"><h4>Further information</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number8" class="toc"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <div style="page-break-before:always">&nbsp;</div>

            <div class="row">
                <div class="col-lg-12">
                    <section class="info-section">
                        <table>
                            <colgroup>
                                <col style="width: 7%">
                                <col style="width: 43%">
                                <col style="width: 50%">
                            </colgroup>
                            <tbody class="text-left">
                                @{
                                    var sec = 1;
                                    var i = sec + 0.1;
                                }
                                <tr class="main-heading toc" id="TOC_Target0">
                                    <td>
                                        <h1>@sec</h1>
                                    </td>
                                    <td colspan="2">
                                        <h1>PERSONAL DETAILS</h1>
                                    </td>
                                </tr>
                                <tr class="info">
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Personal details</h4>
                                        <p class="small-details mb-2"><small>The client submitted the following personal details:</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Title:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.Title</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">First name:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.FirstName</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Middle name(s):</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.MiddleName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Surname:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.Surname</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Previous/maiden name:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.MaidenName</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Known as:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.KnownByName</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Date of birth:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.DateOfBirth</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Sex:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.Gender</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Marital status:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.MaritalStatus</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr class="info">
                                    @{
                                        i = i.IncrementSubSection();
                                    }
                                    <td>@i</td>
                                    <td>
                                        @if (Model.IndividualDetails.ParticipantMainDetail.IsMainApplicant)
                                        {
                                            <h4 class="mb-0">Other applicants details</h4>
                                            <p class="small-details mb-2"><small>This following individual is the primary applicant for this application:</small></p>
                                        }
                                        else
                                        {
                                            <h4 class="mb-0">Joint applicants details</h4>
                                            <p class="small-details mb-2"><small>The client submitted the following details for the spouse/partner as a joint applicant:</small></p>
                                        }
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Title:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointTitle.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">First name:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointFirstName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Middle name(s):</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointMiddleName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Surname:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointSurname.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                @if (Model.IndividualDetails.ParticipantMainDetail.IsMainApplicant)
                                                {
                                                    <tr>
                                                        <td class="mb-0 bold">Email:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointEmail.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td class="mb-0 bold">Previous/maiden name:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointMaidenName.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Known as:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointKnownByName.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Date of birth:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointDateOfBirth.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Sex:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointGender.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Marital status:</td>
                                                        <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointMaritalStatus.ReplaceNullWithHyphen()</td>
                                                    </tr>
                                                }
                                                <tr>
                                                    <td class="mb-0 bold">Relationship:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.JointRelationship.ReplaceNullWithHyphen()</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr class="info">
                                    @{
                                        i = i.IncrementSubSection();
                                    }
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Nationality</h4>
                                        <p class="small-details mb-2"><small>The client submitted the following nationality details:</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Place of Birth:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.PlaceOfBirth.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Town of birth:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.TownOfBirth.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Country of domicile:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.CountryOfDomicile.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Nationality:</td>
                                                    <td class="mb-0">@string.Join(",", Model.IndividualDetails.ParticipantMainDetail.Nationality, Model.IndividualDetails.ParticipantMainDetail.SecondNationality).TrimEnd(',')</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr class="info">
                                    @{
                                        i = i.IncrementSubSection();
                                    }
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Current residential address</h4>
                                        <p class="small-details mb-2"><small>The client submitted the following Current residential address details:</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Flat / apartment sub-building:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialSubBuilding.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Building number:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialBuildingNumber.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Building name:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialBuildingName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Road / street:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialStreet.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Town:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialCity.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">State / Province:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialProvinceName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">State / Province Code:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialProvinceCode.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Post / zip code:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialAddressPostCode.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">District:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialDistrict.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Country:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialCountry.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">From:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.ResidentialAddressFrom</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>

                                @if (Model.IndividualDetails.SummaryPreviousAddresses.Any())
                                {
                                    int a = 1;
                                    foreach (var address in Model.IndividualDetails.SummaryPreviousAddresses)
                                    {
                                        <tr class="info">
                                            @{
                                                i = i.IncrementSubSection();
                                            }
                                            <td>@i</td>
                                            <td>
                                                <h4 class="mb-0">Previous address @a</h4>
                                                <p class="small-details mb-2"><small>The client submitted the following Previous residential address details:</small></p>
                                            </td>
                                            <td>
                                                <table>
                                                    <colgroup>
                                                        <col style="width: 50%">
                                                        <col style="width: 50%">
                                                    </colgroup>
                                                    <tbody>
                                                        <tr>
                                                            <td class="mb-0 bold">Flat / apartment sub-building:</td>
                                                            <td class="mb-0">@address.ResidentialSubBuilding.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Building number:</td>
                                                            <td class="mb-0">@address.ResidentialBuildingNumber.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Building name:</td>
                                                            <td class="mb-0">@address.ResidentialBuildingName.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Road / street:</td>
                                                            <td class="mb-0">@address.ResidentialStreet.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Town:</td>
                                                            <td class="mb-0">@address.ResidentialCity.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">State / Province:</td>
                                                            <td class="mb-0">@address.ResidentialProvinceName.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">State / Province Code:</td>
                                                            <td class="mb-0">@address.ResidentialProvinceCode.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Post / zip code:</td>
                                                            <td class="mb-0">@address.ResidentialAddressPostCode.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">District:</td>
                                                            <td class="mb-0">@address.ResidentialDistrict.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Country:</td>
                                                            <td class="mb-0">@address.ResidentialCountry.ReplaceNullWithHyphen()</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">From:</td>
                                                            <td class="mb-0">@address.ResidentialAddressFrom</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                        a++;
                                    }
                                }

                                <tr class="info">
                                    @{
                                        i = i.IncrementSubSection();
                                    }
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Postal address</h4>
                                        <p class="small-details mb-2"><small>The client submitted the following Postal address details:</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Flat / apartment sub-building:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalSubBuilding.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Building number:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalBuildingNumber.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Building name:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalBuildingName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Road / street:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalStreet.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Town:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalCity.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">State / Province:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalProvinceName.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">State / Province Code:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalProvinceCode.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Post / zip code:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalAddressPostCode.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">District:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalDistrict.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Country:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail?.PostalCountry.ReplaceNullWithHyphen()</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr class="info">
                                    @{
                                        i = i.IncrementSubSection();
                                    }
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Contact details</h4>
                                        <p class="small-details mb-2"><small>The client submitted the following contact details:</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Mobile:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.MobilePhone.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Home:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.HomePhone.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Business:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.BusinessPhone.ReplaceNullWithHyphen()</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Email:</td>
                                                    <td class="mb-0">@Model.IndividualDetails.ParticipantMainDetail.Email.ReplaceNullWithHyphen()</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </section>
                </div>
            </div>
            <div style="page-break-before: always">&nbsp;</div>

            <div class="row">
                <div class="col-lg-12">
                    <section class="info-section">
                        <table>
                            <colgroup>
                                <col style="width: 7%">
                                <col style="width: 43%">
                                <col style="width: 50%">
                            </colgroup>
                            <tbody class="text-left">
                                <tr class="main-heading toc" id="TOC_Target1">
                                    @{
                                        sec += 1;
                                        i = sec + 0.1;
                                    }
                                    <td>
                                        <h1>@sec</h1>
                                    </td>
                                    <td colspan="2" class="heading">
                                        <h1>TAX AND EMPLOYMENT</h1>
                                    </td>
                                </tr>
                                <tr class="info">
                                    <td>@i</td>
                                    <td>
                                        <h4 class="mb-0">Tax residency</h4>
                                        <p class="small-details mb-2"><small>The client has confirmed the following tax residency details.</small></p>
                                    </td>
                                    <td>
                                        <table>
                                            <colgroup>
                                                <col style="width: 50%">
                                                <col style="width: 50%">
                                            </colgroup>
                                            <thead>
                                                <tr style="text-decoration: underline;">
                                                    <th>Country</th>
                                                    <th>Social security/NI number</th>
                                                </tr>
                                            </thead>
                                            <tbody>

                                                @{
                                                    foreach (var residency in Model.IndividualDetails.SummaryTaxDetails)

@model PDF.Models.ViewModels.VerifyViewModel

@using PDF.Models.Enums
@using PDF.PDFGeneration.Helper

@{
    ViewBag.Title = "Verify Report";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Test</title>
    @if (Model.Mode == 2)
    {
        <link rel="stylesheet" href="~/css/bootstrap.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/font-awesome/css/font-awesome.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/custom/verify/verify-report.css" crossorigin="anonymous">
    }
    else
    {
        <link rel="stylesheet" href="css/bootstrap.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="css/font-awesome/css/font-awesome.min.css" crossorigin="anonymous">
        <link rel="stylesheet" href="css/custom/verify/verify-report.css" crossorigin="anonymous">
    }

</head>
<body>
    <div class="body-content">
        <div class="main-container">
            <div class="first-page mt-4">
                <div class="row">
                    <div class="col-lg-6">
                        <h5 class="mb-3">Contact Us</h5>
                        <p class="bold">Online</p>
                        <p class="mb-0">If you need information</p>
                        <p class="mb-0">about our report or</p>
                        <p class="mb-0">services, please visit our</p>
                        <p class="mb-0">knowledge base at</p>
                        <p><a target="_blank" href="https://www.tiller-verify.com">www.tiller-verify.com</a>.</p>

                        <p class="bold">Phone</p>
                        <p class="mb-2">Call us on 020 3196 0365</p>
                        <p class="mb-0">Available 9am - 5pm,</p>
                        <p class="mb-0">Monday to Friday except</p>
                        <p>for bank holidays</p>

                        <p class="bold mb-0">Email</p>
                        <p><a href="mailto:support@tillertech.com">support@tillertech.com</a></p>
                    </div>
                    <div class="col-lg-6">
                        <h1 id="TOC_Target0" class="bookmark toc heading pb-3 mb-5">About this report</h1>
                        @if (Model.ReportServiceType == ReportServiceTypes.EidAndVReport)
                        {
                            @await Html.PartialAsync("PartialView/_AboutEidvReport")
                        }
                        else
                        {
                            @await Html.PartialAsync("PartialView/_AboutVerifyReport")
                        }
                        <p class="pb-3">
                            Our <a href="https://www.tillertech.com/privacy-policy" target="_blank">Privacy Policy</a> is designed to help you
                            understand what kind of information we collect in
                            connection with our services and how we will process
                            and use this Personal Data.
                        </p>

                        <h4 id="TOC_Target1" class="bookmark toc heading">Checks completed</h4>
                        @if (Model.IdAndLiveness != null)
                        {
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Id & Liveness</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(Model.IdAndLiveness.Header.Status.StatusTickIcon())" /> <span class="@(Model.IdAndLiveness.Header.Status.StatusFontColor())"> @Model.IdAndLiveness.Header.Status?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.PepAndSanction != null || Model.PepAndSanctionV2 != null)
                        {
                            var pepSanctionsStatus = Model.PepAndSanction != null ? Model.PepAndSanction.Header.Status : Model.PepAndSanctionV2.Header.Status;
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Pep & Sanctions</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(pepSanctionsStatus.StatusTickIcon())" /> <span class="@(pepSanctionsStatus.StatusFontColor()) pt-1"> @pepSanctionsStatus?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.AdverseMedia != null)
                        {
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Adverse Media</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(Model.AdverseMedia.Header.Status.StatusTickIcon())" /> <span class="@(Model.AdverseMedia.Header.Status.StatusFontColor()) pt-1"> @Model.AdverseMedia.Header.Status?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.AddressVerification != null)
                        {
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Address</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(Model.AddressVerification.Header.Status.StatusTickIcon())" /><span class="@(Model.AddressVerification.Header.Status.StatusFontColor()) pt-2"> @Model.AddressVerification.Header.Status?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.ProofOfAddress != null)
                        {
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Proof of Address</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(Model.ProofOfAddress.Result.StatusTickIcon())" /><span class="@(Model.ProofOfAddress.Result.StatusFontColor()) pt-2"> @Model.ProofOfAddress.Result?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.BankDetail != null)
                        {
                            <div class="row checks-result pb-4 pt-4 pr-0 pl-2 mb-2">
                                <div class="col-md-5">
                                    <p class="mb-0 bold">Bank Account Detail</p>
                                </div>
                                <div class="col-md-7">
                                    <div class="text-right">
                                        <img class="pr-1 pt-0" src="@(Model.BankDetail.Header.Status.StatusTickIcon())" /><span class="@(Model.BankDetail.Header.Status.StatusFontColor()) pt-2"> @Model.BankDetail.Header.Status?.ToUpper()</span>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>

            <div style="page-break-before:always">&nbsp;</div>

            <div class="row">
                <div class="col-lg-12">
                    <div class="section content-section">
                        <h1 class="bold">+ Contents</h1>
                        <hr />
                        <table class="table-of-content">
                            <tbody>
                                <tr>
                                    <td>
                                        <a href="#TOC_Target0" id="TOC_Title0" class="toc contents-page-style"><h4>About this report</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number0" class="toc"></td>
                                </tr>
                                <tr>
                                    <td>
                                        <a href="#TOC_Target1" id="TOC_Title1" class="toc contents-page-style"><h4>Checks completed</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number1" class="toc"></td>
                                </tr>
                                <tr>
                                    <td>
                                        <a href="#TOC_Target2" id="TOC_Title2" class="toc contents-page-style"><h4>Customer information</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number2" class="toc"></td>
                                </tr>
                                <tr>
                                    <td>
                                        <a href="#TOC_Target3" id="TOC_Title3" class="toc contents-page-style sub-content"><h4>Personal details</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number3" class="toc"></td>
                                </tr>
                                @if (Model.CustomerInfo.PersonalDetails.FormerNameDetails?.Count > 0)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target4" id="TOC_Title4" class="toc contents-page-style sub-content"><h4>Former name</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number4" class="toc"></td>
                                    </tr>
                                }
                                @if (Model.CustomerInfo.PersonalDetails.Nationality?.Length > 0)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target5" id="TOC_Title5" class="toc contents-page-style sub-content"><h4>Nationality</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number5" class="toc"></td>
                                    </tr>
                                }
                                <tr>
                                    <td>
                                        <a href="#TOC_Target6" id="TOC_Title6" class="toc contents-page-style sub-content"><h4>Current residential address</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number6" class="toc"></td>
                                </tr>
                                <tr>
                                    <td>
                                        <a href="#TOC_Target7" id="TOC_Title7" class="toc contents-page-style sub-content"><h4>Contact information</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number7" class="toc"></td>
                                </tr>
                                @if (Model.GEOlocation != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target8" id="TOC_Title8" class="toc contents-page-style sub-content"><h4>Geolocation</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number8" class="toc"></td>
                                    </tr>
                                }
                                <tr>
                                    <td>
                                        <a href="#TOC_Target9" id="TOC_Title9" class="toc contents-page-style"><h4>Agreement to terms</h4></a>
                                    </td>
                                    <td id="TOC_Page_Number9" class="toc"></td>
                                </tr>
                                @if (Model.IdAndLiveness != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target10" id="TOC_Title10" class="toc contents-page-style"><h4>ID & Liveness - @Model.IdAndLiveness.Header.Status.StatusDescription()</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number10" class="toc"></td>
                                    </tr>

                                    <tr>
                                        <td>
                                            <a href="#TOC_Target11" id="TOC_Title11" class="toc contents-page-style sub-content"><h4>Verified Document</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number11" class="toc"></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target12" id="TOC_Title12" class="toc contents-page-style sub-content"><h4>Liveness</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number12" class="toc"></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target13" id="TOC_Title13" class="toc contents-page-style sub-content"><h4>Verify Face Match</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number13" class="toc"></td>
                                    </tr>
                                }
                                @if (Model.PepAndSanction != null || Model.PepAndSanctionV2 != null)
                                {
                                    var pepSanctionsStatus = Model.PepAndSanction != null ? Model.PepAndSanction.Header.Status : Model.PepAndSanctionV2.Header.Status;
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target14" id="TOC_Title14" class="toc contents-page-style"><h4>PEP & Sanctions Screening - @pepSanctionsStatus.StatusDescription()</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number14" class="toc"></td>
                                    </tr>
                                }

                                @if (Model.AdverseMedia != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target15" id="TOC_Title15" class="toc contents-page-style"><h4>Adverse Media - @Model.AdverseMedia.Header.Status.StatusDescription()</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number15" class="toc"></td>
                                    </tr>
                                }

                                @if (Model.AddressVerification != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target16" id="TOC_Title16" class="toc contents-page-style"><h4>Address Verification - @Model.AddressVerification.Header.Status.StatusDescription()</h4></a>
                                        </td>
                                        <td id="TOC_Page_Number16" class="toc"></td>
                                    </tr>
                                }

                                @if (Model.BankDetail != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target17" id="TOC_Title17" class="toc contents-page-style"><h4>Bank Account Details - @Model.BankDetail.Header.Status.StatusDescription()</h4></a>
                                        </td>
                                    </tr>
                                }

                                @if (Model.SourceOfFunds != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target18" id="TOC_Title18" class="toc contents-page-style"><h4>Source of Funds - @Model.SourceOfFunds.Status.StatusDescription()</h4></a>
                                        </td>
                                    </tr>
                                }

                                @if (Model.ProofOfAddress != null)
                                {
                                    <tr>
                                        <td>
                                            <a href="#TOC_Target19" id="TOC_Title19" class="toc contents-page-style"><h4>Proof of address – @Model.ProofOfAddress.Result.ProofOfAddressStatusDescription()</h4></a>
                                        </td>
                                    </tr>
                                }
                                <tr>
                                    <td>
                                        <a href="#TOC_Target20" id="TOC_Title20" class="toc contents-page-style"><h4>Further information</h4></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <div style="page-break-before:always">&nbsp;</div>

            <div class="row">
                <div class="col-lg-12">
                    <section class="customer-info-section">
                        <div class="customer-info-header">
                            <h1 id="TOC_Target2" class="bookmark toc heading">
                                Customer information
                            </h1>
                        </div>
                        <table class="detail-info mb-4">
                            <colgroup>
                                <col style="width:30%">
                                <col style="width:20%">
                            </colgroup>
                            <tbody class="text-left">
                                <tr class="customer-info">
                                    <td>
                                        <h4 id="TOC_Target3" class="bookmark toc heading mb-0 mt-2">Personal details</h4>
                                        <p class="small-details-customer mb-2"><small>The customer submitted the following personal details:</small></p>
                                    </td>
                                    <td>
                                        <table class="mb-2 mt-2">
                                            <colgroup>
                                                <col style="width:50%">
                                                <col style="width:50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">First name:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.PersonalDetails.FirstName</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Middle name(s):</td>
                                                    <td class="mb-0">@Model.CustomerInfo.PersonalDetails.MiddleName</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Surname:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.PersonalDetails.Surname</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Date of birth:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.PersonalDetails.DateOfBirth</td>
                                                </tr>
                                                <tr>
                                                    <td class="mb-0 bold">Sex:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.PersonalDetails.Sex</td>
                                                </tr>
                                                @if (!string.IsNullOrEmpty(Model.CustomerInfo.PersonalDetails.PlaceOfBirth))
                                                {
                                                    <tr>
                                                        <td class="mb-0 bold">Place of birth:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.PersonalDetails.PlaceOfBirth</td>
                                                    </tr>
                                                }

                                                @if (!string.IsNullOrEmpty(Model.CustomerInfo.PersonalDetails.CountryOfBirth))
                                                {
                                                    <tr>
                                                        <td class="mb-0 bold">Country of birth:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.PersonalDetails.CountryOfBirth</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                @if (Model.CustomerInfo.PersonalDetails.FormerNameDetails?.Count > 0)
                                {
                                    <tr class="customer-info">
                                        <td>
                                            <h4 id="TOC_Target4" class="bookmark toc heading mb-0 mt-2">Former name</h4>
                                            <p class="small-details-customer mb-2"><small>The customer submitted the following former name details:</small></p>
                                        </td>
                                        <td>
                                            <table class="mb-2 mt-2">
                                                <colgroup>
                                                    <col style="width:50%">
                                                    <col style="width:50%">
                                                </colgroup>
                                                <tbody>

                                                    @foreach (var detail in Model.CustomerInfo.PersonalDetails.FormerNameDetails)
                                                    {
                                                        <tr>
                                                            <td class="mb-0 bold">Former name:</td>
                                                            <td class="mb-0">@detail.FormerName</td>
                                                        </tr>
                                                        <tr>
                                                            <td class="mb-0 bold">Former name change date:</td>
                                                            <td class="mb-0">@detail.ChangeDate</td>
                                                        </tr>
                                                    }

                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                }
                                @if (Model.CustomerInfo.PersonalDetails.Nationality?.Length > 0)
                                {
                                    <tr class="customer-info">
                                        <td>
                                            <h4 id="TOC_Target5" class="bookmark toc heading mb-0 mt-2">Nationality</h4>
                                            <p class="small-details-customer mb-2"><small>The customer provided the following nationality information:</small></p>
                                        </td>
                                        <td>
                                            <table class="mb-2 mt-2">
                                                <colgroup>
                                                    <col style="width:50%">
                                                    <col style="width:50%">
                                                </colgroup>
                                                <tbody>
                                                    @{
                                                        var i = 1;
                                                        foreach (var detail in Model.CustomerInfo.PersonalDetails.Nationality)
                                                        {
                                                            var heading = i == 1 ? "Nationality:" : "";
                                                            <tr>
                                                                <td class="mb-0 bold">@heading</td>
                                                                <td class="mb-0">@detail</td>
                                                            </tr>
                                                            i++;
                                                        }
                                                    }
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                }
                                @if (Model.CustomerInfo.CurrentResidentialAddress != null)
                                {
                                    <tr class="customer-info">
                                        <td>
                                            <h4 id="TOC_Target6" class="bookmark toc heading mb-0 mt-2">Current residential address</h4>
                                            <p class="small-details-customer mb-2"><small>The customer submitted the following address:</small></p>
                                        </td>
                                        <td>
                                            <table class="mb-2 mt-2">
                                                <colgroup>
                                                    <col style="width:50%">
                                                    <col style="width:50%">
                                                </colgroup>
                                                <tbody>
                                                    <tr>
                                                        <td class="mb-0 bold">Flat / apartment sub-building:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.FlatApartmentSubBuilding</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Building number:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.BuildingNumber</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Building name:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.BuildingName</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Road / street:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.RoadStreet</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Town:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.Town</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">State / Province:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.StateProvince</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">State / Province Code:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.StateProvinceCode</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Post / zip code:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.PostZipCode</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">District:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.District</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Country:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.Country</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">From:</td>
                                                        <td class="mb-0">@Model.CustomerInfo.CurrentResidentialAddress.From</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                }
                                <tr class="customer-info">
                                    <td>
                                        <h4 id="TOC_Target7" class="bookmark toc heading mb-0 mt-2">Contact information</h4>
                                        <p class="small-details-customer mb-2"><small>The customer has provided the following contact information:</small></p>
                                    </td>
                                    <td>
                                        <table class="mb-2 mt-2">
                                            <colgroup>
                                                <col style="width:50%">
                                                <col style="width:50%">
                                            </colgroup>
                                            <tbody>
                                                <tr>
                                                    <td class="mb-0 bold">Email:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.ContactInformation.Email</td>
                                                </tr>
                                                @*          <tr>
                                                    <td class="mb-0 bold">Mobile telephone:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.ContactInformation.MobileTelephone</td>
                                                    </tr>
                                                    <tr>
                                                    <td class="mb-0 bold">Landline telephone:</td>
                                                    <td class="mb-0">@Model.CustomerInfo.ContactInformation.LandlineTelephone</td>
                                                    </tr> *@
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                @if (Model.GEOlocation != null)
                                {
                                    <tr class="customer-info">
                                        <td>
                                            <h4 id="TOC_Target8" class="bookmark toc heading mb-0 mt-2">Geolocation</h4>
                                            @if (!string.IsNullOrEmpty(Model.GEOlocation.Latitude) && !string.IsNullOrEmpty(Model.GEOlocation.Longitude))
                                            {
                                                <p class="small-details-customer mb-2"><small>The GPS location was retrieved from the customer’s device. You can view the exact location using the following <a href="https://www.google.com/maps/search/?api=1&query=@(Model.GEOlocation.Latitude)%2C@(Model.GEOlocation.Longitude)" target="_blank">link</a>.</small></p>
                                            }
                                            else
                                            {
                                                <p class="small-details-customer mb-2"><small>No geolocation data supplied. Customer did not approve access.</small></p>
                                            }
                                        </td>
                                        <td>
                                            <table class="mb-2 mt-2">
                                                <colgroup>
                                                    <col style="width:50%">
                                                    <col style="width:50%">
                                                </colgroup>
                                                <tbody>
                                                    <tr>
                                                        <td class="mb-0 bold">Status:</td>
                                                        <td class="mb-0">
                                                            <img class="pr-1" src="@(Model.GEOlocation.Status.StatusTickIcon())" />
                                                            <span> @Model.GEOlocation.Status?.ToUpper()</span>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="mb-0 bold">Latitude:</td>
                                                        <td class="mb-0">@Model.GEOlocation.Latitude</td>
